{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["POST"]
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 3009,
      "maxDuration": 30
    }
  }
}
